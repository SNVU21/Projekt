@page
@model WeekModel
@{
    ViewData["Title"] = "Wochenübersicht";
    var weeks = Model.Weeks.ToList();
    var CurrentMonth = DateTime.Now.ToString("MMMM") +" "+ DateTime.Today.ToString("yyyy");
    DateTime erster = new DateTime(2020, 1, 1);
    DateTime start = erster.AddDays((7 * (weeks[0].id - 1)) + 5);
    DateTime end = start.AddDays(4);
}
<style>
    td {
        height: 40px;
        width: 80px;
        padding: 50px;
        margin: 50px auto;
        border: 3px solid gray;
        border-radius: 10px;
        font-size: 24px;
        text-align: center;
     }
</style>
<h1>@ViewData["Title"]</h1>

<p>Übersicht der Wochen vom @CurrentMonth</p>
